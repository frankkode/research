{
  "name": "research-study-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@types/node": "^16.18.68",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "ajv": "6.12.6",
    "ajv-keywords": "3.5.2",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-pdf": "^10.0.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "react-scripts": "^5.0.1",
    "recharts": "^3.1.0",
    "serve": "^14.2.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --collectCoverageFrom='src/**/*.{ts,tsx}' --collectCoverageFrom='!src/**/*.d.ts' --collectCoverageFrom='!src/index.tsx' --collectCoverageFrom='!src/setupTests.ts' --watchAll=false",
    "test:ci": "react-scripts test --coverage --collectCoverageFrom='src/**/*.{ts,tsx}' --collectCoverageFrom='!src/**/*.d.ts' --collectCoverageFrom='!src/index.tsx' --collectCoverageFrom='!src/setupTests.ts' --watchAll=false --passWithNoTests",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css,json}",
    "format:check": "prettier --check src/**/*.{ts,tsx,css,json}",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.0.11",
    "prettier": "^3.1.0"
  },
  "proxy": "http://localhost:8001",
  "resolutions": {
    "ajv": "6.12.6",
    "ajv-keywords": "3.5.2"
  }
}
